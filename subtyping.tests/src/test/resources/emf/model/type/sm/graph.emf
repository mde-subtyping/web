@namespace(uri="metamodel.graph", prefix="graph")
package graph;

class Graph {
  attr String name;
  val Node[*] nodes;
  val Edge[*] edges;
}

class Node {
  attr String name;
}

class Edge {
  attr String name;
  ref Node[1] source;
  ref Node[1] target;
}

datatype String : java.lang.String;

datatype Boolean : boolean;

