context Edge
inv map:
not(Edge.allInstances()->exists(e | 
	e.source=self.source
	and
	e.target<>self.target 
))

context Node
inv nodeContainment:
StateMachine.allInstances()->exists(sm | sm.nodes->includes(self))

context Edge
inv edgeContainment:
StateMachine.allInstances()->exists(sm | sm.edges->includes(self))
